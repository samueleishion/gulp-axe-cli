{
  "name": "gulp-axe-cli",
  "version": "1.0.0-alpha.1",
  "description": "Gulp plugin to test static pages with aXe as pre-commit hook or in CI environment.",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/samueleishion/gulp-axe-cli",
  "author": {
    "name": "Sam Acuna",
    "email": "samueleishion@gmail.com",
    "url": "samuelacuna.com"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "standard ./**/*.js --fix",
    "test": "npm-run-all lint \"gulp test\"",
    "axe": "axe",
    "gulp": "gulp",
    "node": "node",
    "standard": "standard",
    "npm-run-all": "npm-run-all"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "gulp",
    "plugin",
    "axe",
    "a11y",
    "accessibility",
    "testing",
    "automation",
    "semantic",
    "html",
    "wcag",
    "wcag2a",
    "wcag2aa",
    "wai",
    "aria"
  ],
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "axe-cli": "^3.2.0",
    "fancy-log": "^1.3.3",
    "plugin-error": "^1.0.1",
    "through2": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "ava": "^2.4.0",
    "gulp": "^4.0.0",
    "gulp-ava": "^2.0.0",
    "gulp-live-server": "0.0.31",
    "husky": "^3.0.8",
    "npm-run-all": "^4.1.3",
    "p-event": "^4.1.0",
    "standard": "^14.3.1",
    "vinyl": "^2.1.0"
  },
  "standard": {
    "ignore": [
      "node_modules",
      ".git"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "npm run lint",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-push": "npm-run-all test"
    }
  }
}
